@model RoleTOP_MVC.ViewModels.DashboardViewModel
<main>
    <div class="resumo-adm">
            <div>
                <p class="titulo-resumo">@Model.clienteEspecifico.Email</p>
            </div>
            <div class="info-evento">
                <table>
                <tr>
                    <th>Nome do Usuário:</th><td>@Model.clienteEspecifico.Nome</td>
                </tr>
                <tr>
                    <th>CEP:</th><td>@Model.clienteEspecifico.CEP</td>
                </tr>
                <tr>
                    <th>CPF:</th><td>@Model.clienteEspecifico.CPF</td>
                </tr>
                <tr>
                    <th>Telefone:</th><td>@Model.clienteEspecifico.Tel</td>
                </tr>
            </table>
            </div>
            <div class="btn">
                <a href="javascript:history.back()">Voltar à página anterior</a>
            </div>
        </div>
</main>